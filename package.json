{
	"version": "1.0.0",
	"private": true,
	"name": "nuxt-course",
	"description": "Udemy course by Maximilian Schwarzm√ºller about Nuxt.js 2 Framework - Vue.js on Steroids. Build highly engaging Vue JS apps with Nuxt.js. Nuxt adds easy server-side-rendering and a folder-based config approach.",
	"author": "beatrizsmerino@gmail.com",
	"license": "ISC",
	"type": "module",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"prepare": "husky install",
		"dev": "npm run env:package && nuxt",
		"build": "npm run env:package && nuxt build",
		"start": "npm run env:package && nuxt start",
		"generate": "npm run env:package && nuxt generate",
		"prettier:fix": "prettier --write \"./**/*.{css,scss,sass,vue,js,json}\" ./.prettierrc",
		"eslint:fix": "eslint \"./**/*.{vue,js,json}\" --fix --ignore-path ./.eslintignore",
		"stylelint:fix": "stylelint \"./**/*.{css,scss,sass,vue}\" --fix --ignore-path ./.stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"test": "jest",
		"env:package": "node env-package.js"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"body-parser": "^1.20.2",
		"core-js": "^3.36.1",
		"express": "^4.19.2",
		"js-cookie": "^3.0.5",
		"nuxt": "^2.16.0",
		"vue": "^2.7.14",
		"vue-server-renderer": "^2.7.16",
		"vue-template-compiler": "^2.7.16",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.24.3",
		"@babel/eslint-parser": "^7.24.1",
		"@babel/plugin-syntax-import-assertions": "^7.24.1",
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.3",
		"@nuxt/babel-preset-app": "^2.17.3",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.2",
		"@nuxtjs/stylelint-module": "^4.2.2",
		"@vue/test-utils": "^1.3.6",
		"autoprefixer": "^10.4.19",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^29.7.0",
		"browserslist": "^4.23.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^27.9.0",
		"eslint-plugin-jsonc": "^2.14.1",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.24.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"postcss": "^8.4.38",
		"postcss-html": "^1.6.0",
		"postcss-import": "^15.1.0",
		"prettier": "^3.2.5",
		"sass": "^1.72.0",
		"sass-loader": "^10.4.1",
		"stylelint": "^16.4.0",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-sass-guidelines": "^11.1.0",
		"stylelint-config-standard": "^36.0.0",
		"vue-jest": "^3.0.7"
	},
	"resolutions": {
		"glob-parent": "6.0.2",
		"json5": "2.2.3",
		"nth-check": "2.1.1"
	},
	"engines": {
		"node": "20.11.0",
		"npm": "10.4.0"
	}
}
