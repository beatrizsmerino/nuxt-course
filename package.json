{
	"version": "2.7.0",
	"private": true,
	"name": "nuxt-course",
	"description": "Udemy course by Maximilian Schwarzm√ºller about Nuxt.js 2 Framework - Vue.js on Steroids. Build highly engaging Vue JS apps with Nuxt.js. Nuxt adds easy server-side-rendering and a folder-based config approach.",
	"author": "beatrizsmerino@gmail.com",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/beatrizsmerino/nuxt-course.git"
	},
	"bugs": {
		"url": "https://github.com/beatrizsmerino/nuxt-course/issues"
	},
	"keywords": [
		"nuxt",
		"vue",
		"ssr",
		"javascript",
		"udemy"
	],
	"type": "module",
	"scripts": {
		"install:clean": "rm -rf node_modules package-lock.json",
		"dependencies:fix": "npm install --ignore-scripts && npx npm-force-resolutions",
		"preinstall": "npm run install:clean && npm run dependencies:fix",
		"postinstall": "npm run lint",
		"prepare": "husky",
		"dev": "npm run env:package && nuxt",
		"build": "npm run env:package && nuxt build",
		"start": "npm run env:package && nuxt start",
		"generate": "npm run env:package && nuxt generate",
		"prettier:fix": "prettier --write \"./**/*.{css,scss,sass,json,js,cjs,mjs,vue}\" ./.prettierrc",
		"eslint:fix": "eslint \"./**/*.{json,js,cjs,mjs,vue}\" --fix --ignore-path ./.eslintignore",
		"stylelint:fix": "stylelint \"./**/*.{css,scss,sass,vue}\" --fix --ignore-path ./.stylelintignore",
		"lint": "npm run prettier:fix && npm run eslint:fix && npm run stylelint:fix",
		"test": "jest",
		"env:package": "node env-package.js"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.13.6",
		"@nuxtjs/svg-sprite": "^0.5.2",
		"body-parser": "^2.2.2",
		"core-js": "^3.48.0",
		"express": "^5.2.0",
		"js-cookie": "^3.0.5",
		"nuxt": "^2.18.1",
		"vue": "^2.7.14",
		"vue-server-renderer": "^2.7.16",
		"vue-template-compiler": "^2.7.16",
		"vuex": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.28.6",
		"@babel/eslint-parser": "^7.28.6",
		"@commitlint/cli": "^20.4.0",
		"@commitlint/config-conventional": "^20.3.1",
		"@nuxt/babel-preset-app": "^2.18.1",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-module": "^3.1.0",
		"@nuxtjs/style-resources": "^1.2.2",
		"@nuxtjs/stylelint-module": "^4.2.2",
		"@vue/test-utils": "^1.3.6",
		"autoprefixer": "^10.4.23",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^30.2.0",
		"browserslist": "^4.28.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jest": "^29.12.1",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-vue": "^9.32.0",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"lint-staged": "^16.2.7",
		"postcss": "^8.5.6",
		"postcss-html": "^1.8.1",
		"postcss-import": "^16.1.1",
		"prettier": "^3.8.1",
		"sass": "^1.97.3",
		"sass-loader": "^10.4.1",
		"stylelint": "^16.26.1",
		"stylelint-config-property-sort-order-smacss": "^11.1.0",
		"stylelint-config-recommended-vue": "^1.6.1",
		"stylelint-config-sass-guidelines": "^12.1.0",
		"stylelint-config-standard": "^39.0.1",
		"stylelint-scss": "^6.14.0",
		"vue-jest": "^3.0.7"
	},
	"resolutions": {
		"glob-parent": "6.0.2",
		"json5": "2.2.3",
		"nth-check": "2.1.1"
	},
	"overrides": {
		"@commitlint/top-level": {
			"find-up": {
				"path-exists": "^5.0.0"
			}
		},
		"read-pkg-up": {
			"find-up": "^4.1.0"
		},
		"braces": "^3.0.3",
		"micromatch": "^4.0.8",
		"postcss": "^8.5.6",
		"cookie": "^0.7.2",
		"devalue": "^5.3.2",
		"tmp": "^0.2.4",
		"@babel/traverse": "^7.28.0",
		"axios": "^0.28.0",
		"http-proxy-middleware": "^3.0.5",
		"nth-check": "^2.1.1",
		"vue-jest": {
			"babel-plugin-transform-es2015-modules-commonjs": {
				"babel-template": {
					"babel-traverse": "npm:@babel/traverse@^7.28.0"
				}
			}
		}
	},
	"engines": {
		"node": "24.13.0",
		"npm": "11.6.2"
	}
}
